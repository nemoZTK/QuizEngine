{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": "Debug",
    "WriteTo": [
      { "Name": "Console" },
      {
        "Name": "File",
        "Args": {
          "path": "./Logs/QuizEngineLog-.txt",
          "rollingInterval": "Day",
          "retainedFileCountLimit": 7
        }
      }
    ],
    "Properties": {
      "Application": "QUIZ_ENGINE"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "QuizDb": "Server=<server>;Database=<db_name>;User Id=<user>;Password=<pwd>;TrustServerCertificate=True;"
  }
}
